<!--pages/maps/maps.wxml-->
<map style="width:100%;height:900rpx" markers="{{markers}}" bindcallouttap="bindcallouttap" longitude="{{markers[0].longitude}}" latitude="{{markers[0].latitude}}"></map>
<button type="primary" size="mini" bindtap="searchArea">查看附近的核酸检测点</button>
<button type="primary" size="mini" bindtap="getPlace">去附近最佳检测点</button>